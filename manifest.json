{
  "manifest_version": 2,

  "name": "startit: google docs quick starter",
  "description": "A simple extension to make it easier to create a new Google document.",
  "version": "1.2",
  "icons": { "128": "img/logo.png" },
  "web_accessible_resources": ["img/logo.png", "img/docs_logo.png", "img/sheets_logo.png", "img/slides_logo.png"],
  "browser_action": {
    "default_icon": "img/logo.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"

}
