{
  "manifest_version": 2,

  "name": "startit",
  "description": "Start working and stop wasting time trying to create a document/spreadsheet/slidedeck again!",
  "version": "1.0",
  "icons": { "128": "img/icon.png" },
  "web_accessible_resources": ["img/icon.png", "img/docs_logo.png", "img/sheets_logo.png", "img/slides_logo.png"],
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"

}
