{
  "manifest_version": 2,

  "name": "startit: start working without the delay",
  "description": "Need to get to work on your Google document? Let us help you startit.",
  "version": "1.0",
  "icons": { "128": "img/logo.png" },
  "web_accessible_resources": ["img/logo.png", "img/docs_logo.png", "img/sheets_logo.png", "img/slides_logo.png"],
  "browser_action": {
    "default_icon": "img/logo.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"

}
